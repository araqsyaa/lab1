// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int func(int n1, int n2){
    int k;
    if(n1==n2)
    return n1;
     else if (n1>n2){
        for(int i=1; i<=n2; i++){
            if(n1%i==0 && n2%i==0)
            k=i;
        }
    }
     else if (n1<n2){
        for(int i=1; i<=n1; i++){
          if(n1%i==0 && n2%i==0)
            k=i;  
        }
    }
    return k;
}

int main() {
    int num1, num2;
    cout<<"Enter number 1: ";
    cin>>num1;
    cout<<endl<<"Enter number 2: ";
    cin>>num2;
    if(num1<0)
    num1*=-1;
    if(num2<0)
    num2*=-1;
    cout<<func(num1, num2);
    return 0;
}
